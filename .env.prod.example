 GNU nano 7.2 .env.prod *
# --- Variables de la aplicación Flask (Backend) ---
FLASK_ENV=production
# --- Credenciales de Supabase (necesarias para el backend) ---
SUPABASE_URL='xxxxx'
SUPABASE_SERVICE_KEY='xxxxx'
SUPABASE_JWT_SECRET='xxxxx'
VITE_SUPABASE_URL=xxxxx
VITE_SUPABASE_ANON_KEY=xxxxx
# --- URLs de Webhook de n8n (para el backend) ---
N8N_WEBHOOK_URL_INGESTA_DI='http://n8n:5678/webhook/xxxxx'
N8N_WEBHOOK_URL_VALIDATE_DI='http://n8n:5678/webhook/xxxxx'
N8N_WEBHOOK_URL_CONSULTA_DI='http://n8n:5678/webhook/xxxxx'
N8N_WEBHOOK_URL_SYNC_DOMAIN='http://n8n:5678/webhook/xxxxx'
N8N_WEBHOOK_URL_SYNC_VOCABULARY='http://n8n:5678/webhook/xxxxx'
N8N_WEBHOOK_URL_ANALYZE_ALIGNMENT='http://n8n:5678/webhook/analyze-alignment'
N8N_GENERATE_INDICATORS_URL='http://n8n:5678/webhook/xxxxx'
# --- Configuración de la Base de Datos de n8n (PostgreSQL) ---
POSTGRES_DB=n8n_db_prod
POSTGRES_USER=n8n_user_prod
POSTGRES_PASSWORD=una_contraseña_muy_segura_para_postgres_prod
# --- Configuración de n8n Headless ---
N8N_OWNER_EMAIL=xxxx
N8N_OWNER_PASSWORD=xxxxx
N8N_ENCRYPTION_KEY=xxxx
GENERIC_TIMEZONE=America/Santiago
# Desactiva temporalmente la cookie segura para permitir login vía HTTP
N8N_SECURE_COOKIE=false
N8N_PROXY_MODE=true
WEBHOOK_URL=xxxx/n8n/
N8N_INTERNAL_URL=http://n8n:5678/
