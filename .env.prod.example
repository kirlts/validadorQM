# .env.prod.example
# Variables necesarias para el entorno de producción.
# Estos valores deben ser gestionados como secretos en el pipeline de CI/CD.

# --- Backend ---
SUPABASE_URL=
SUPABASE_SERVICE_KEY=
SUPABASE_JWT_SECRET=
# URL interna para que el backend hable con n8n
N8N_INTERNAL_URL=http://n8n:5678
# ... (y cualquier otra variable que necesite el backend en prod)

# --- Base de Datos de n8n ---
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=

# --- Configuración de n8n ---
N8N_OWNER_EMAIL=
N8N_OWNER_PASSWORD=
N8N_ENCRYPTION_KEY=
GENERIC_TIMEZONE=America/Santiago